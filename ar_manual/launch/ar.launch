<launch>
    <node pkg = "joy" name = "joy" type = "joy_node"/>
    <node pkg = "ar_manual" name = "main_manual" type = "main_manual"/>
    <node pkg = "ar_manual" name = "mecanum_manual" type = "mecanum_manual"/>
    <node pkg = "ar_manual" name = "servo_handler_manual" type = "servo_handler_manual"/>
    <node pkg = "rosserial_python" type = "serial_node.py" name = "nhk18_ar1">
      <param name = "port" type = "string" value = "/dev/ttyACM0"/>
    </node>
    <node pkg = "rosserial_python" type = "serial_node.py" name = "nhk18_ar2">
      <param name = "port" type = "string" value = "/dev/ttyACM1"/>
    </node>
</launch>
